<html>
  <head>
    <title><%= title %></title>
  </head>
  <body>
    <%- include('navbar', {links: links}) %>
    <ul>
      <% for (let i=0; i<messages.length; i++) { %>
        <li>
          <%= messages[i].text %>
          -<i><%= messages[i].user %></i>
          <%= messages[i].added %>
          <a href="/message/<%= messages[i].id%>">Details</a>
        </li>
      <% } %>
    </ul>
  </body>
</html>